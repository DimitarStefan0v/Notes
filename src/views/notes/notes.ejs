<%- include('../includes/head.ejs') %>
</head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <h1>Notes Page</h1>
            <% if (notes.length> 0) { %>
                <ul>
                    <% for (let note of notes) { %>
                        <li>
                            <%= note %>
                        </li>
                        <% } %>
                </ul>
                <% } else { %>
                    <h2>You don't have any notes yes!</h2>
                    <% } %>
                    
        </main>
    </body>

</html>